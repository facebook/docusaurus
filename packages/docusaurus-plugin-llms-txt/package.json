    "@docusaurus/utils-validation": "3.8.1",
    "fs-extra": "^11.1.1",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "commander": "^5.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=20.0"
